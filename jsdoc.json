{
	"tags": {
		"allowUnknownTags": false
	},
	"source": {
		"include": ".",
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|docs)"
	},
	"plugins": [
		"plugins/markdown"
	],
	"opts": {
		"template": "node_modules/docdash",
		"encoding": "utf8",
		"destination": "docs/",
		"recurse": true,
		"verbose": true
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"docdash": {
		"search": true,
		"sectionOrder": [ 
			"Modules",
			"Classes",
			"Externals",
			"Events",
			"Namespaces",
			"Mixins",
			"Tutorials",
			"Interfaces"
	  ],
	  "menu":{                      
			"FWCloud.net":{         
				"href":"https://fwcloud.net", 
				"target":"_blank",
				"class":"menu-item",
				"id":"website_link"
			}
		}
	}
}
