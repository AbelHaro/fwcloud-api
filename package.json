{
  "name": "FWCloud-API",
  "version": "1.9.2",
  "description": "FWCloud RESTful API.",
  "private": true,
  "scripts": {
    "start": "npm run stop; node dist/src/bin/fwcloud",
    "start:bg": "npm start &",
    "stop": "if [ -f \".pid\" ]; then PID=$(cat .pid); EXISTS=`ps axw | awk '{print $1}' | grep \"^${PID}$\"`; if [ \"$EXISTS\" ]; then kill $PID; fi; fi",
    "update": "npm run stop && git checkout -- package-lock.json && git pull && npm install && git checkout -- package-lock.json && npm run build && node fwcli migration:run && node fwcli standard:services:add",
    "update-cert": "bin/update-cert.sh api",
    "generate-jsdoc": "node_modules/.bin/jsdoc -c jsdoc.json",
    "generate-apidoc": "node_modules/.bin/apidoc -i routes -o apidoc/",
    "clean": "rm -fr dist/",
    "build": "npm run clean && mkdir -p dist/src && cp -r src/views dist/src/views && node_modules/.bin/tsc",
    "watch": "npm run build -- -w",
    "fwcloud": "node --no-warnings dist/src/cli/cli.js",
    "test": "npm run build && NODE_ENV=test node --no-warnings node_modules/mocha/bin/_mocha --timeout 99999 --exit --colors dist/tests/**/*spec.js"
  },
  "apidoc": {
    "title": "FWCloud.net API",
    "url": "https://api.fwcloud.net",
    "template": {
      "forceLanguage": "en",
      "withCompare": false,
      "withGenerator": false
    }
  },
  "dependencies": {
    "apidoc": "^1.2.0",
    "archiver": "^7.0.1",
    "async": "^3.2.5",
    "async-mutex": "^0.5.0",
    "axios": "^0.21.4",
    "bcrypt-cli": "^1.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "busboy": "^0.3.1",
    "chalk": "^4.1.2",
    "child-process-promise": "^2.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "convict": "^6.2.4",
    "convict-format-with-moment": "^6.2.0",
    "convict-format-with-validator": "^6.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^1.8.3",
    "dotenv": "^16.4.5",
    "easy-tree": "0.0.3",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "fancy-log": "2.0.0",
    "form-data": "^4.0.0",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "http-proxy": "^1.18.1",
    "ip": "^2.0.1",
    "joi": "^14.3.1",
    "method-override": "^3.0.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "mysql2": "^3.9.7",
    "openpgp": "4.10.10",
    "passport": "^0.7.0",
    "passport-local": "1.0.0",
    "qrcode": "^1.5.3",
    "reflect-metadata": "0.2.2",
    "semver": "7.6.2",
    "semver-compare": "^1.0.0",
    "session-file-store": "^1.5.0",
    "shell-escape": "^0.2.0",
    "socket.io": "^3.0.4",
    "speakeasy": "^2.0.0",
    "sqlstring": "2.3.3",
    "ssh2": "^1.15.0",
    "ts-node": "^8.6.2",
    "typeorm": "^0.2.30",
    "typescript": "^3.9.10",
    "uuid": "9.0.1",
    "when": "^3.7.8",
    "winston": "^3.8.2",
    "ws": "^8.17.0",
    "yargs": "^15.4.1",
    "yauzl": "^3.1.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/chai-as-promised": "^7.1.3",
    "@types/chai-json-schema": "^1.4.6",
    "@types/cron": "^1.7.1",
    "@types/mocha": "10.0.6",
    "@types/node": "13.5.0",
    "@types/session-file-store": "^1.2.5",
    "@types/sinon": "^7.5.2",
    "@types/supertest": "2.0.8",
    "@types/yargs": "15.0.1",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-json-schema": "^1.5.1",
    "cookie-signature": "1.2.1",
    "mocha": "^10.4.0",
    "sinon": "9.0.0",
    "supertest": "4.0.2"
  }
}
