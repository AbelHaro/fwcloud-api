{
  "name": "FWCloud.net-API",
  "version": "0.8.5",
  "description": "Documentation for the RESTful API of the FWCloud.net project.",
  "private": true,
  "scripts": {
    "start": "npm run build && node dist/src/bin/www.js",
    "generate-jsdoc": "node_modules/.bin/jsdoc -c jsdoc.json",
    "generate-apidoc": "node_modules/.bin/apidoc -i routes -o apidoc/",
    "build": "node_modules/.bin/tsc",
    "fwcloud": "node_modules/.bin/ts-node cli/cli.ts",
    "migration:run": "npm run build && node dist/src/cli/cli.js migration:run",
    "migration:revert": "npm run build && node dist/src/cli/cli.js migration:revert",
    "migration:reset": "npm run build && node dist/src/cli/cli.js migration:reset",
    "migration:create": "npm run build && node dist/src/cli/cli.js migration:create",
    "test": "jest"
  },
  "apidoc": {
    "title": "FWCloud.net API",
    "url": "https://api.fwcloud.net",
    "template": {
      "forceLanguage": "en",
      "withCompare": false,
      "withGenerator": false
    }
  },
  "dependencies": {
    "apidoc": "^0.19.0",
    "async": "^3.1.0",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "bookshelf": "^1.0.1",
    "child-process-promise": "^2.2.1",
    "compression": "^1.7.4",
    "convict": "^5.2.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dateformat": "^3.0.3",
    "debug": "^4.1.1",
    "docdash": "^1.1.1",
    "dotenv": "^8.2.0",
    "easy-tree": "0.0.3",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "formats": "^1.0.0",
    "fs": "*",
    "fs-extra": "^8.1.0",
    "helmet": "^3.21.2",
    "ip": "^1.1.5",
    "is-ip": "^3.1.0",
    "jade": "~1.11.0",
    "joi": "^14.3.1",
    "jsdoc": "^3.6.3",
    "lockfile": "^1.0.4",
    "lodash": "^4.17.15",
    "log4js": "~6.1.0",
    "log4js-extend": "^0.2.1",
    "method-override": "^3.0.0",
    "moment-timezone": "^0.5.27",
    "morgan": "~1.9.1",
    "mysql": "^2.17.1",
    "mysql-import": "^3.0.6",
    "mysqldump": "^3.1.0",
    "node-datetime": "^2.1.2",
    "passport": "^0.4.1",
    "passport-local": "*",
    "proper-lockfile": "^4.1.1",
    "random-string": "^0.2.0",
    "replace-in-file": "^4.2.0",
    "serve-favicon": "~2.5.0",
    "session-file-store": "^1.3.1",
    "socket.io": "^2.3.0",
    "sse-channel": "^3.1.1",
    "ssh2": "^0.8.7",
    "ts-node": "^8.6.2",
    "typeorm": "^0.2.22",
    "typescript": "^3.7.4",
    "when": "^3.7.8",
    "xml2js": "^0.4.22",
    "yargs": "^15.1.0",
    "yuidocjs": "^0.10.2"
  },
  "devDependencies": {
    "@types/jest": "25.1.0",
    "@types/node": "13.5.0",
    "@types/yargs": "15.0.1",
    "jest": "25.1.0",
    "ts-jest": "25.0.0"
  }
}
